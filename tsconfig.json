{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es2017",
      "es6",
      "dom"
    ],
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "baseUrl": "../",
    "typeRoots": [
      "../"
    ],
    "paths": {
      "@dsinjs/*": [
        "dsinjs__*"
      ]
    },
    "types": [],
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "files": [
    "src/index.ts"
  ]
}